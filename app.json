{
  "expo": {
    "name": "BioDex",
    "slug": "BioDex",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "biodex",
    "icon": "src/assets/iconLay.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.biodex"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/iconLay.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.biodex"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}